{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist/types",
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "ES6",
    "moduleResolution": "node",
    "outDir": "./dist",
    "plugins": [{
      "name": "typescript-plugin-css-modules"
    }],
    "strict": true
  },
  "exclude": [
    "dist/**/*",
    "node_modules"
  ]
}

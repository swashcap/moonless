{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "jsx": "react",
    "outFile": "dist/index.js",
    "plugins": [{
      "name": "typescript-plugin-css-modules"
    }],
    "strict": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
